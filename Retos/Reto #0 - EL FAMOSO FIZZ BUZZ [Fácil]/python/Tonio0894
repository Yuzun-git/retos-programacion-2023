def multiploambos (nro):
    return (nro % 3 == 0) and (nro % 5 == 0)

def multiplode3 (nro):
    return (nro % 3 == 0)

def multiplode5 (nro):
    return (nro % 5 == 0)

def fizzbuzz():

    multiplo3 = 'fizz'
    multiplo5 = 'buzz'

    for i in range(1,101):
        if multiploambos(i):
            print(mult3 + mult5)
        elif multiplode3(i):
            print(mult3)
        elif multiplode5(i):
            print(mult5)
        else:
            print(i)

fizzbuzz()
